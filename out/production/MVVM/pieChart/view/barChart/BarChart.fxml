<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pieChart.view.barChart.BarChartController">
   <children>
      <BarChart fx:id="barChart" layoutX="-1.0" layoutY="41.0" prefHeight="373.0" prefWidth="600.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
      <Button layoutX="130.0" layoutY="14.0" mnemonicParsing="false" onAction="#onUpdateButton" text="Update" />
      <Label fx:id="update" layoutX="14.0" layoutY="18.0" prefHeight="17.0" prefWidth="93.0" text="Label" />
   </children>
</AnchorPane>
